<!doctype html>
<html lang="en">

<head>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1">

	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
		integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	<link rel="stylesheet" href="/css/spicy.css">

	<title>Spicy Battle</title>
	<!-- Google Analytics -->
	<script>
	(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
	(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
	m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
	})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

	ga('create', 'UA-159381533-1', 'auto');
	ga('require', 'GTM-N67KLQD');
	ga('send', 'pageview');
	</script>
	<!-- End Google Analytics -->
</head>

<body>
			<div class="container">

				<h1 class="title_header">BATTLE</h1>


				<div class="window_frame">
					<div class="window">

						<img class="monster_battle_placement" src="/images/monster-2.png"/>
						<div class="progress monster-hp">
							<div id="monster_hp" class="progress-bar hp_bar" role="progressbar" style="width: 100%; border: 5px;" aria-valuenow="25"
								aria-valuemin="0" aria-valuemax="100"></div>
						</div>
						<img id="character" class="character_battle_placement" src="/images/character-battle-2.png"/>
					</div>
				</div>

				{{#pushups}}
				<div class="row">

					<div class="col-6 text-center" style="margin: 0 auto; margin-top: 10px">
						<button style="width: 160px; margin-top: 10px; font-size: 12px; text-transform: uppercase"
						type="button" onclick="location.href='/PushUps';"
						class="btn btn-dark btn-lg exercise-button">{{exerciseName}} <span class="badge badge-secondary">{{repetitions}}</span>
					</button>
					</div>
				{{/pushups}}
				{{#situps}}


					<div class="col-6 text-center" id={{id}} style="margin: 0 auto; margin-top: 10px">
						<button style="width: 160px; margin-top: 10px; font-size: 12px; text-transform: uppercase"
						type="button" onclick="location.href='/SitUps';"
						class="btn btn-dark btn-lg exercise-button">{{exerciseName}} <span class="badge badge-secondary">{{repetitions}}</span>
					</button>

					</div>
				</div>
				{{/situps}}
				{{#jumpingjacks}}
				<div class="row">

					<div class="col-6 text-center" id={{id}} style="margin: 0 auto; margin-top: 10px">
						<button style="width: 160px; margin-top: 10px; font-size: 12px; text-transform: uppercase"
						type="button" onclick="location.href='/JumpingJacks';"
						class="btn btn-dark btn-lg exercise-button">{{exerciseName}} <span class="badge badge-secondary">{{repetitions}}</span>
						</button>
					</div>



				{{/jumpingjacks}}
				{{#squats}}

					<div class="col-6 text-center" id={{id}} style="margin: 0 auto; margin-top: 10px">
						<button style="width: 160px; margin-top: 10px; font-size: 12px; text-transform: uppercase"
						type="button" onclick="location.href='/Squats';"
						class="btn btn-dark btn-lg exercise-button">{{exerciseName}} <span class="badge badge-secondary">{{repetitions}}</span>
						</button>
					</div>
				</div>
				{{/squats}}
				<div >
					<div class="run-button-container" style="text-align: center;">
						<button style="width: 100px; margin-top: 10px; font-size: 10px; text-transform: uppercase"
						type="button" onclick="location.href='/index';"
						class="btn btn-dark btn-lg exercise-button">RUN AWAY</button>
					</div>
				</div>

			</div>
			<script>
			$(".exercise-button").click(function(){
				$(".monster_battle_placement").effect("shake",{times:4});
			});
			</script>

	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	<script src="https://code.jquery.com/jquery-3.4.1.slim.min.js"
		integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n"
		crossorigin="anonymous"></script>
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
		integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
		crossorigin="anonymous"></script>
	<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"
		integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6"
		crossorigin="anonymous"></script>
	<script src="https://code.jquery.com/jquery.js"></script>
	<script type = "text/javascript"
         src = "https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js">
      </script>

      <script type = "text/javascript"
         src = "https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.3/jquery-ui.min.js">
      </script>
	<script src="/js/fight.js"></script>
	<script src="/js/battle.js"></script>
</body>

</html>
